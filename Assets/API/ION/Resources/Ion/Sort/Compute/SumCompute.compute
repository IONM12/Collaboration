StructuredBuffer<int> Sum1Buffer;
RWStructuredBuffer<int> Sum2Buffer;
//---------------------------------------------------------------------------------------------
[numthreads(1,1,1)]
//---------------------------------------------------------------------------------------------
void Main (uint3 id : SV_DispatchThreadID)
{
	int index = id.x;
	
	int sum = 0;

	for (int i = 0; i < 256; i++)
	{
		Sum2Buffer[i] = sum;

		sum += Sum1Buffer[i];
	}
}
